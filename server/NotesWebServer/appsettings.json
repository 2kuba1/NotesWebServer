{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "connectionString": "yourDbConnectionString"
  },

  "Jwt": {
    "Issuer": "http://webnotes.com",
    "Audience": "http://webnotes.com",
    "Key": "PRIVATE_KEY_DONT_SHEAR",
    "ExpireDays": 15
  },

  "Storage":{
    "BlobStorageConnectionString": "yourBlobStorageConnectionString",
    "FilesBlobStorageContainerName": "files"
  }
}
